#: pdf_bot/dispatcher.py:113
msgid "Welcome to PDF Bot!"
msgstr "Vítejte v PDF Bot!"

#: pdf_bot/dispatcher.py:114
msgid "Key features:"
msgstr "Klíčové vlastnosti:"

#: pdf_bot/dispatcher.py:115
msgid "- Compress, merge, preview, rename, split and add watermark to PDF files"
msgstr "- Komprimovat, sloučit, zobrazit náhled, přejmenovat, rozdělit a přidat vodoznak do souborů PDF"

#: pdf_bot/dispatcher.py:119
msgid "- Create PDF files from text messages"
msgstr "- Vytváření souborů PDF z textových zpráv"

#: pdf_bot/dispatcher.py:120
msgid "- Extract images and text from PDF files"
msgstr "- Extrahování obrázků a textu ze souborů PDF"

#: pdf_bot/dispatcher.py:121
msgid "- Convert PDF files into images"
msgstr "- Převod souborů PDF na obrazy"

#: pdf_bot/dispatcher.py:122
msgid "- Convert webpages and images into PDF files"
msgstr "- Převod webových stránek a obrázků do souborů PDF"

#: pdf_bot/dispatcher.py:123
msgid "- Beautify handwritten notes images into PDF files"
msgstr "- Zkrášlujte ručně psané obrázky poznámek do souborů PDF"

#: pdf_bot/dispatcher.py:124
msgid "- And more..."
msgstr "- A další..."

#: pdf_bot/dispatcher.py:125
msgid "Type {command} to see how to use PDF Bot"
msgstr "Zadáním {command} , jak používat PDF Bota"

#: pdf_bot/dispatcher.py:137
msgid "Set Language 🌎"
msgstr "Nastavit jazyk 🌎"

#: pdf_bot/dispatcher.py:139 pdf_bot/utils.py:278
msgid "Join Channel"
msgstr "Připojit se k kanálu"

#: pdf_bot/dispatcher.py:140 pdf_bot/payment.py:85 pdf_bot/utils.py:279
msgid "Support PDF Bot"
msgstr "Podpora PDF Bot"

#: pdf_bot/dispatcher.py:149
msgid "You can perform most of the tasks by sending me one of the followings:"
msgstr "Většinu úkolů můžete provést zasláním jedné z následujících akcí:"

#: pdf_bot/dispatcher.py:152
msgid "- PDF files"
msgstr "- Soubory PDF"

#: pdf_bot/dispatcher.py:153
msgid "- Images"
msgstr "- Obrázky"

#: pdf_bot/dispatcher.py:154
msgid "- Webpage links"
msgstr "- Odkazy na webové stránky"

#: pdf_bot/dispatcher.py:155
msgid "The rest of the tasks can be performed by using the following commands:"
msgstr "Zbytek úkolů lze provést pomocí následujících příkazů:"

#: pdf_bot/dispatcher.py:159
msgid "{command} - compare PDF files"
msgstr "{command} - porovnat soubory PDF"

#: pdf_bot/dispatcher.py:160
msgid "{command} - merge PDF files"
msgstr "{command} - sloučit soubory PDF"

#: pdf_bot/dispatcher.py:161
msgid "{command} - convert and combine multiple images into PDF files"
msgstr "{command} - převést a kombinovat více obrázků do souborů PDF"

#: pdf_bot/dispatcher.py:164
msgid "{command} - create PDF files from text messages"
msgstr "{command} - vytváření souborů PDF z textových zpráv"

#: pdf_bot/dispatcher.py:167
msgid "{command} - add watermark to PDF files"
msgstr "{command} - přidání vodoznaku do souborů PDF"

#: pdf_bot/feedback.py:48
msgid "Send me your feedback (only English feedback will be forwarded to my developer)"
msgstr "Pošlete mi svůj názor (pouze anglická zpětná vazba bude předána mému vývojáři)"

#: pdf_bot/feedback.py:83
msgid "Thank you for your feedback, I've already forwarded it to my developer"
msgstr "Děkujeme vám za vaši zpětnou vazbu, už jsem ji předal svému vývojáři"

#: pdf_bot/feedback.py:87
msgid "The feedback is not in English, try again"
msgstr "Zpětná vazba není v angličtině, zkuste to znovu"

#: pdf_bot/language.py:28
msgid "Select your language"
msgstr "Vyberte si jazyk"

#: pdf_bot/language.py:73
msgid "Your language has been set to {language}"
msgstr "Váš jazyk byl nastaven na {language}"

#: pdf_bot/payment.py:58
msgid "Help translate PDF Bot"
msgstr "Nápověda přeložit PDF Bot"

#: pdf_bot/payment.py:70
msgid "Select how you want to support PDF Bot"
msgstr "Vyberte, jak chcete podporovat PDF Bot"

#: pdf_bot/payment.py:86
msgid "Say thanks to PDF Bot and help keep it running"
msgstr "Řekněte díky PDF Bot a pomozte udržet ho v chodu"

#: pdf_bot/payment.py:107
msgid "Something went wrong"
msgstr "Něco se pokazilo"

#: pdf_bot/payment.py:115
msgid "Thank you for your support!"
msgstr "Děkujeme vám za vaši podporu!"

#: pdf_bot/url.py:29
msgid "Timed out processing your web page, your web page is probably too complex to process"
msgstr "Vypršel časový limit zpracování vaší webové stránky, vaše webová stránka je pravděpodobně příliš složitá na zpracování"

#: pdf_bot/url.py:36
msgid "You've sent me this web page already and I'm still converting it"
msgstr "Už jsi mi poslal tuhle webovou stránku a já ji pořád převádím"

#: pdf_bot/url.py:39
msgid "Converting your web page into a PDF file"
msgstr "Převedení webové stránky na soubor PDF"

#: pdf_bot/url.py:51
msgid "Unable to reach your web page"
msgstr "Nelze se dostat na webovou stránku"

